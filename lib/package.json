{
  "name": "@agent-os/lib",
  "version": "1.0.0",
  "description": "Common TypeScript library for agent-os scripts",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    },
    "./1password": {
      "types": "./dist/1password/index.d.ts",
      "import": "./dist/1password/index.js"
    },
    "./llm": {
      "types": "./dist/llm/index.d.ts",
      "import": "./dist/llm/index.js"
    },
    "./git": {
      "types": "./dist/git/index.d.ts",
      "import": "./dist/git/index.js"
    }
  },
  "scripts": {
    "build": "bun build ./index.ts --outdir ./dist --target bun",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.0",
    "chalk": "^5.4.1",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.5",
    "typescript": "^5.7.3"
  }
}
